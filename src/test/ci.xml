<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">


<suite name="Test Suite">
    <parameter name="url" value="${URL}"/>
    <parameter name="mainLogin" value="${MAIN_LOGIN}"/>
    <parameter name="mainPassword" value="${MAIN_PASSWORD}"/>
    <parameter name="adminLogin" value="${ADMIN_LOGIN}"/>
    <parameter name="adminPassword" value="${ADMIN_PASSWORD}"/>
    <parameter name="attachFolder" value="${ATTACH_FOLDER}"/>
    <parameter name="headless" value="${HEADLESS}"/>
    <parameter name="no_sandbox" value="${NO_SANDBOX}"/>
    <parameter name="disable_gpu" value="${DISABLE_GPU}"/>
    <parameter name="remote_url" value="${REMOTE_URL}"/>
    <test name="CI-TEST">
        <parameter name="attachFolder" value="./build/attachTests"/>
        <groups>
            <run>
                <include name = "ALL"/>
                <include name = "PROXY"/>
                <include name = "CleanUp"/>
            </run>
        </groups>
        <packages>
            <package name="dicts" />
            <package name="events" />
            <package name="map" />
            <package name="map" />
            <package name="pmi_tests" />
            <package name="reports" />
        </packages>
    </test>
</suite>