            <class name="dicts.CheckingFiles"></class>
            <class name="dicts.Dicts_CumulativePlans"></class>
            <class name="dicts.SearchTests"></class>
            <class name="dicts.Labels"></class>
            <class name="dicts.Dicts_Incidents"></class>
            <class name="dicts.Dicts_KB"></class>
            <class name="events.Events_Incidents"></class>
            <class name="events.StatusTests"></class>
            <class name="map.MapTest"></class>
            <class name="pmi_tests.PimTests"></class>
            <class name="reports.Reports"></class>
            <class name="CleanUp"></class>
            <class name="PlayGround"></class>
        </classes-->
